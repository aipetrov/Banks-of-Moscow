<Window x:Name="SignInWindow" x:Class="Banks_of_Moscow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Banks_of_Moscow"
        mc:Ignorable="d"
        Title="Вход" Height="257.996" Width="397.9">
    <Grid>
        <Label x:Name="labellogin" Content="Логин:" HorizontalAlignment="Left" Margin="89,14,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxlogin" HorizontalAlignment="Left" Height="23" Margin="89,40,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="210"/>
        <Label x:Name="labelpassword" Content="Пароль:" HorizontalAlignment="Left" Margin="89,67,0,0" VerticalAlignment="Top"/>
        <Button x:Name="buttonsignin" Content="Войти" HorizontalAlignment="Left" Margin="149,126,0,0" VerticalAlignment="Top" Width="75" Click="buttonsignin_Click"/>
        <Button x:Name="buttonregister" Content="Зарегистрироваться" HorizontalAlignment="Left" Margin="122,175,0,0" VerticalAlignment="Top" Width="129" Height="24" Click="buttonregister_Click"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="89,98,0,0" VerticalAlignment="Top" Width="210"/>

    </Grid>
</Window>
